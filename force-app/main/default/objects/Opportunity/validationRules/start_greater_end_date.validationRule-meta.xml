<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>start_greater_end_date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    Flight_Start_Date__c &gt; Flight_End_Date__c,
    NOT(Skip_Validations__c)
)</errorConditionFormula>
    <errorDisplayField>Flight_End_Date__c</errorDisplayField>
    <errorMessage>End date should be greater than start date</errorMessage>
</ValidationRule>
